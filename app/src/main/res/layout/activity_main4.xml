<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity4">

    <ImageView
        android:id="@+id/logo3"
        android:layout_width="177dp"
        android:layout_height="96dp"
        android:layout_marginTop="92dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ecomind" />

    <TextView
        android:id="@+id/cliquenomapa"
        android:layout_width="292dp"
        android:layout_height="154dp"
        android:layout_marginTop="40dp"
        android:hint="@string/cliquenomapa"
        android:textColorHint="#000000"
        app:layout_constraintEnd_toEndOf="@+id/logo3"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="@+id/logo3"
        app:layout_constraintTop_toBottomOf="@+id/logo3" />

    <ImageView
        android:id="@+id/googlemaps"
        android:layout_width="363dp"
        android:layout_height="308dp"
        android:layout_marginTop="208dp"
        app:layout_constraintEnd_toEndOf="@+id/logo3"
        app:layout_constraintStart_toStartOf="@+id/logo3"
        app:layout_constraintTop_toBottomOf="@+id/logo3"
        app:srcCompat="@drawable/gm" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_popup_reminder" />

    <Button
        android:id="@+id/irParaPerfil"
        android:layout_width="47dp"
        android:layout_height="36dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>